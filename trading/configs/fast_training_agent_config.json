            "sde_sample_freq": -1,
            "device": "cpu",
            "rollout_buffer_class": null,
            "rollout_buffer_kwargs": {},
            "target_kl": null,
            "stats_window_size": null,
            "policy_kwargs": {
                "net_arch": [
                    {"pi": [256, 256], "vf": [256, 256]}
                ]
            },
            "verbose": 2,
            "seed": 42
    }
