{
    "algo": "ppo",
    "total_timesteps": 1e5,
    "save_path": "{PROJECT_ROOT}/models/ppo_fast_training",
        "kwargs": {
            "policy": "MlpPolicy",
            "learning_rate": {
                "type": "linear",
                "initial_value": 0.0003
